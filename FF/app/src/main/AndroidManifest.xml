<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farmfresh">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".RegisterActivity"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".AddAutoActivity"></activity>
        <activity android:name=".IndexActivity"></activity>
        <activity android:name=".CurrentOrdersActivity"></activity>
        <activity android:name=".PreviousOrdersActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".SupportActivity"></activity>


    </application>

</manifest>